# Frontend only (set these in Vercel project env settings)
# Point to your named tunnel/API hostname (not localhost / not trycloudflare quick URL).
VITE_API_BASE=https://plot-reproduce-timeline-recommended.trycloudflare.com
VITE_API_KEY=4a00372e2f061de230a70ef8303411ff

# Metadata transport
VITE_WS_METADATA_URL=wss://plot-reproduce-timeline-recommended.trycloudflare.com/api/realtime/ws
VITE_METADATA_LATEST_URL=https://plot-reproduce-timeline-recommended.trycloudflare.com/api/realtime/latest
VITE_METADATA_POLL_MS=250

# Preferred direct media path (non-backend)
# Option A: Janus gateway (recommended for RTSP cameras)
VITE_JANUS_HTTP_URL=https://get-basin-exceed-jan.trycloudflare.com/janus
VITE_JANUS_MOUNTPOINT=1

# Option B: direct media URL (HLS/MJPEG/WebRTC gateway URL)
# Note: raw rtsp:// links are NOT browser-playable.
# If using image-like MJPEG URL, set kind=image; otherwise keep auto.
VITE_DIRECT_STREAM_URL=
VITE_DIRECT_STREAM_KIND=auto

# When true, frontend will not use backend WebRTC/MJPEG media fallbacks.
VITE_DISABLE_BACKEND_VIDEO=true
VITE_DISABLE_WEBRTC=false
